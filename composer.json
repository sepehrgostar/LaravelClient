{
    "name": "sepehrgostar/LaravelClient",
    "description": ":package_description",
    "license": "license",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/sepehrgostar/laravelClient",
    "keywords": ["Laravel", "Api"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Sepehrgostar\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sepehrgostar\\Api\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sepehrgostar\\LaravelClient\\laravelClientServiceProvider"
            ],
            "aliases": {
                "Api": "Sepehrgostar\\LaravelClient\\Facades\\Api"
            }
        }
    }
}
